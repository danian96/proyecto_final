
<br>
<div class="col-lg-12">
    <div class="ibox float-e-margins">
        <div class="ibox-title">
            <h5>Designacion</h5>
        </div>
        <div class="ibox-content">
          <p id="notice"><%= notice %></p>
          <p>
            <strong>Name:</strong>
            <%= @designation.name %>
          </p>
        <p>
          <strong>Departamento:</strong>
          <%= @designation.departament.name %>
        </p>


      <%= link_to 'Editar', edit_designation_path(@designation), class:"btn btn-sm btn-white"%>
      <%= link_to 'Atras', designations_path , class:"btn btn-sm btn-white"%>
    </div>
  </div>
</div>
