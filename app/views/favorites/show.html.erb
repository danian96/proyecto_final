
<br>
<div class="col-lg-12">
    <div class="ibox float-e-margins">
      <p id="notice"><%= notice %></p>
        <div class="ibox-title">
            <h5>Perfil</h5>
        </div>
        <div class="ibox-content">
        <p>
          <strong>Name:</strong>
          <%= @favorite.name %>
        </p>

        <p>
          <strong>Acctionlink:</strong>
          <%= @favorite.acctionlink %>
        </p>

        <p>
          <strong>User:</strong>
          <%= @favorite.user.email %>
        </p>

        <%= link_to 'Editar',edit_favorite_path(@favorite) , class:"btn btn-sm btn-white"  %>
        <%= link_to 'Volver', favorites_path , class:"btn btn-sm btn-white"  %>
      </div>
  </div>
</div>
